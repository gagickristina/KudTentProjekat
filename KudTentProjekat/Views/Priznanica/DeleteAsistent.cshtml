@model KudTentProjekat.Models.Priznanica

@{
    ViewData["Title"] = "DeleteAsistent";
}



<div class="jumbotron">
    <h3>Da li ste sigurni da želite da izbrišete priznanicu?</h3>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Id:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Idpriznanice)
        </dd>
        <dt class="col-sm-2">
            Član ID:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Idkorisnika)
        </dd>

        <dt class="col-sm-2">
            Tip priznanice:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NazivMeseca)
        </dd>
        <dt class="col-sm-2">
            Datum:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Datum)
        </dd>

    </dl>

    <form asp-action="DeleteAsistent">
        <input type="submit" value="Delete" class="btn btn-danger" />
        |
        <a asp-action="IndexAsistent">Back</a>

    </form>

</div>
